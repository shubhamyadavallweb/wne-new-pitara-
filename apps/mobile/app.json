{
  "expo": {
    "name": "Pitara",
    "slug": "pitara",
    "version": "1.0.0",
    "entryPoint": "./index.js",
    "orientation": "portrait",
    "icon": "./assets/Assets/pitaralogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Assets/pitaralogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
